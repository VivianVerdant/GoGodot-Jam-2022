config_version=4

[application]

config/name="GoGodot Jam 2022"
run/main_scene="res://rooms/test_room.tscn"
config/icon="res://icon.png"

[autoload]

PlayerInput="*res://player/player_input.gd"
WindowManager="*res://window_manager.gd"

[display]

window/size/width=480
window/size/height=320
window/size/test_width=960
window/size/test_height=640
window/stretch/mode="viewport"

