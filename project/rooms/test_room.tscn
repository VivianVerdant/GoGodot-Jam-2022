script/source = "extends KinematicBody2D

var dxdy = Vector2.ZERO
var sleeping = true
var target

var initial_pos

const SPEED = 25

func _ready():
	initial_pos = position
	target = get_tree().get_nodes_in_group(\"player\")[0]

func _physics_process(_delta):
	if sleeping:
		return
	dxdy = global_position.direction_to(target.global_position).normalized() * SPEED 
	move_and_slide(dxdy)

func reset():
	sleeping = true
	position = initial_pos
"

